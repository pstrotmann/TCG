<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC
 "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
 "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>

	<application>
		<locale-config>
			<default-locale id="defDe">de</default-locale>
			<supported-locale id="supDe">de</supported-locale>
		</locale-config>
	</application>

	<managed-bean>
		<managed-bean-name>anmelder</managed-bean-name>
		<managed-bean-class>org.strotmann.tcg.Anmelder</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>mgr</managed-bean-name>
		<managed-bean-class>org.strotmann.tcg.MitgliedManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
		
	<managed-bean>
		<managed-bean-name>mitglied</managed-bean-name>
		<managed-bean-class>org.strotmann.tcg.Mitglied</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>mSuch</managed-bean-name>
		<managed-bean-class>org.strotmann.tcg.MitgliedSucher</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>auswahl</managed-bean-name>
		<managed-bean-class>org.strotmann.util.Auswahl</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<navigation-rule>
		<from-view-id>/pages/anmeldung.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Erfolg</from-outcome>
			<to-view-id>/pages/liste.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Fehler</from-outcome>
			<to-view-id>/pages/anmeldung.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
		
	<navigation-rule>
		<from-view-id>/pages/liste.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Neu</from-outcome>
			<to-view-id>/pages/erfassung.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Aendern</from-outcome>
			<to-view-id>/pages/erfassung.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Uebernahme</from-outcome>
			<to-view-id>/pages/liste.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Suchen</from-outcome>
			<to-view-id>/pages/liste.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Beitraege</from-outcome>
			<to-view-id>/pages/beitragsliste.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Adressen</from-outcome>
			<to-view-id>/pages/adressliste.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Mails</from-outcome>
			<to-view-id>/pages/mailAnListe.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Export</from-outcome>
			<to-view-id>/pages/liste.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>KijuUmst</from-outcome>
			<to-view-id>/pages/beitragsliste.jsp</to-view-id>
		</navigation-case>
		</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/erfassung.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Erfolg</from-outcome>
			<to-view-id>/pages/liste.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>Fehler</from-outcome>
			<to-view-id>/pages/erfassung.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/adressliste.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Erfolg</from-outcome>
			<to-view-id>/pages/liste.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>ListeKompr</from-outcome>
			<to-view-id>/pages/adressliste.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/beitragsliste.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Erfolg</from-outcome>
			<to-view-id>/pages/liste.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/mailAnListe.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Erfolg</from-outcome>
			<to-view-id>/pages/liste.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>ListeKompr</from-outcome>
			<to-view-id>/pages/mailAnListe.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
  <converter>
	<description>
		increments date by one to overcome outputText Problem
	</description>
	<converter-id>datumConverter</converter-id>
	<converter-class>
		org.strotmann.util.DatumConverter
	</converter-class>
  </converter>
  
  <converter>
	<description>
		1 --> Lastschrift, 2 --> Ãœberweisung
	</description>
	<converter-id>zahlungsartConverter</converter-id>
	<converter-class>
		org.strotmann.util.ZahlungsartConverter
	</converter-class>
  </converter>
  
  <converter>
	<description>
		1 --> EinzelBeitrag, 2 --> Ehepaar, 3 -->
	</description>
	<converter-id>beitragsklasseConverter</converter-id>
	<converter-class>
		org.strotmann.util.BeitragsklasseConverter
	</converter-class>
  </converter>
  
  <converter>
	<description>
		1 --> 240,00, 2 --> 410,00, 3 -->
	</description>
	<converter-id>beitragConverter</converter-id>
	<converter-class>
		org.strotmann.util.BeitragConverter
	</converter-class>
  </converter>
  
	
</faces-config>
